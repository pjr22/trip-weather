OPENROUTESERVICE_API_KEY=`cat ../openRouteService_api_key.txt`
GEOAPIFY_API_KEY=`cat ../geoApify_api_key.txt`
echo "Using OpenRouteService API key: $OPENROUTESERVICE_API_KEY"
echo "Using GeoApify API key: $GEOAPIFY_API_KEY"
docker run --name trip-weather \
 -p 9080:8080 -d \
 -e SERVER_PORT=8080 \
 -e OPENROUTESERVICE_API_KEY=${OPENROUTESERVICE_API_KEY} \
 -e GEOAPIFY_API_KEY=${GEOAPIFY_API_KEY} \
 --net forgotten_net \
 org.pjr22/trip-weather:latest 
